{
  "name": "minggas-portolio",
  "version": "1.0.0",
  "description": "Minggas portfolio",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass -w scss/ -o dist/css/ --recursive",
    "babel": "babel --plugins transform-es2015-arrow-functions js/main.js --watch --out-file dist/js/main.js",
    "dev": "concurrently --kill-others \"npm run sass\" \"npm run babel\""
