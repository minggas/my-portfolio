    "frontend"
  ],
  "author": "minggas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minggas/my-portfolio/issues"
  },
  "homepage": "https://minggas.github.io/my-portfolio",
  "devDependencies": {
    "node-sass": "^4.9.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "concurrently": "^3.6.1"
  }
}